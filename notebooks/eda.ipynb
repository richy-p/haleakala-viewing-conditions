    "def count_NaNs(df):\n",
    "    len_df = len(df)\n",
    "    max_digits = int(np.log10(len_df) + 1)\n",
    "    print(f'Total rows          : {len_df}')\n",
    "    print('-----------------------------')\n",
    "    print('Number of NaNs per column:')\n",
    "    for col in df:\n",
    "        print(f'{col:20}: {sum(df[col].isna()):{max_digits}}')\n",
    "\n"
   ]
  },
